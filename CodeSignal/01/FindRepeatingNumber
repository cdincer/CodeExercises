   public  int firstDuplicate(int[] a)
    {
        Set<Integer> result = new HashSet();
        for(int i=0; i<a.length; i++)
        {
            if(result.contains(a[i]))
            {
                return a[i];
            } else
            {

                result.add(a[i]);
            }
        }
        return -1;
    }